page {
  height: 100%;
  background-color: #FDD30F;
}
.withdraw {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 98vh;
  background: white;
  border-radius: 60rpx 60rpx 0px 0px;
}
.detail {
  width: 654rpx;
  height: 403rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  margin: 91rpx auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.detail .title {
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #310800;
  margin-top: 46rpx;
}
.detail .inputBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 506rpx;
  border-bottom: 1px solid #ddd;
  padding-bottom: 18rpx;
  margin-top: 140rpx;
}
.detail .inputBox .t1 {
  font-size: 36rpx;
  font-weight: bold;
}
.detail .inputBox .input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.detail .inputBox .t2 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: black;
}
.detail .desc {
  width: 506rpx;
  margin-top: 26rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.detail .desc .t1 {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: black;
}
.detail .desc .t2 {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #21b0f9;
}
.btn {
  position: absolute;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  bottom: 128rpx;
  width: 610rpx;
  height: 88rpx;
  background: #fdd30f;
  border-radius: 44rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: black;
}

