.search {
  position: relative;
  width: 100%;
  padding-bottom: 23rpx;
  background: #fdd30f;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.search .input {
  width: 690rpx;
  height: 54rpx;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 6rpx;
  padding-left: 40rpx;
  box-sizing: border-box;
}
.search .plClass {
  text-align: center;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}
.search .ssIcon {
  width: 30rpx;
  height: 28rpx;
  position: absolute;
  top: 12rpx;
  left: 272rpx;
}
.search .focusClass {
  left: 32rpx;
}
.title {
  width: 690rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-top: 38rpx;
}
.title .t1 {
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: black;
}
.title .del {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.title .del .delIcon {
  width: 24rpx;
  height: 24rpx;
  margin-right: 8rpx;
}
.title .del text {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #666666;
}
.history {
  width: 690rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin-top: 40rpx;
}
.history .item {
  padding: 19rpx 33rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: black;
  margin-right: 20rpx;
  margin-bottom: 30rpx;
  background: #f2f2f2;
  border-radius: 4rpx;
}
.result {
  width: 690rpx;
}

