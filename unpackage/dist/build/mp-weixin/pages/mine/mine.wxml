<view class="main"><view class="header" style="{{'padding-top:'+(top+'px')+';'}}"><view class="title">我的</view></view><view class="info"><view class="content"><block wx:if="{{isLogin==false}}"><view class="noLogin"><button class="getUserBtn" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e"></button><view class="imgBox"><image src="../../static/icon/mine/noLogin.png" mode="aspectFit"></image></view><text>授权登入</text></view></block><block wx:else><view class="top"><image class="avatar" src="{{userInfo.avatar}}" mode="aspectFill"></image><view class="right"><view class="row"><text class="name">{{userInfo.name}}</text><block wx:if="{{userInfo.exp==userInfo.userLevel.exp}}"><text data-event-opts="{{[['tap',[['goTest',['$event']]]]]}}" class="up" bindtap="__e">去考试？</text></block><block wx:else><text data-event-opts="{{[['tap',[['goUp',['$event']]]]]}}" class="up" bindtap="__e">去升级？</text></block></view><view class="line"><text></text></view><view class="detail"><view class="item"><image class="ykIcon" src="../../static/icon/yk.png" mode="aspectFit"></image><text class="text">游客</text></view><view class="item"><image class="vipIcon" src="{{imgUrl+userInfo.userLevel.icon}}" mode="aspectFit"></image><text class="text">{{"成为"+userInfo.userLevel.name}}</text></view><view class="item"><text>经验值</text><text class="jyNum">{{userInfo.exp||0}}</text><text>{{"/"+userInfo.userLevel.exp}}</text></view></view></view></view></block><view class="bottom"><view data-event-opts="{{[['tap',[['goCommission',['$event']]]]]}}" class="item" bindtap="__e"><text class="count">{{userInfo.money||0}}</text><text class="desc">我的佣金</text></view><view data-event-opts="{{[['tap',[['goIntegral',['$event']]]]]}}" class="item" bindtap="__e"><text class="count">{{userInfo.point||0}}</text><text class="desc">会员积分</text></view><view data-event-opts="{{[['tap',[['goCollect',['$event']]]]]}}" class="item" bindtap="__e"><text class="count">{{userInfo.storeCount}}</text><text class="desc">我的收藏</text></view></view></view></view><view class="ad"><image class="icon" src="../../static/icon/medal.png" mode="aspectFit"></image><view class="detail"><text class="t1">会员学院</text><text class="t2">参与会员学院学习，获得更多权益</text></view><view data-event-opts="{{[['tap',[['goSchool',['$event']]]]]}}" class="btn" bindtap="__e">立即学习</view></view><view class="titleBox">我的工具</view><view class="moduleList"><block wx:for="{{iconText}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['goPage',[index]]]]]}}" class="item" bindtap="__e"><view class="iconbox"><block wx:if="{{index==0&&userInfo.messageCount>0}}"><text class="msgNum">{{userInfo.messageCount}}</text></block><block wx:if="{{index==8}}"><text class="hb">火爆</text></block><image src="{{'../../static/icon/mine/icon'+(index+1)+'.png'}}" mode="aspectFit"></image></view><text>{{item}}</text></view></block></view><tab-bar vue-id="bae1c640-1" tabIndex="{{4}}" bind:__l="__l"></tab-bar></view>